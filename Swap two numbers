import java.util.*;
public class swap {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.print("Enter a number(first num):");
        int num1=sc.nextInt();
        System.out.print("Enter other number(second num):");
        int num2=sc.nextInt();
        int temp=num1;
        num1=num2;
        num2=temp;
        System.out.println("The first number after swapping is :"+num1);
        System.out.println("The second number after swapping is :"+num2);
       
    }
    
}
