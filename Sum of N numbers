import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
		System.out.print("Enter a Number: ");
		int num=sc.nextInt();
		int sum=0;
		for(int i=1;i<=num;i++){
		    sum+=i;
		}
		System.out.print("Sum of first "+num+" natural numbers is "+sum);
	}
}
